{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "baseUrl": "./",
    "paths": {
      "@routes/*": ["src/routes/*"],
      "@middlewares/*": ["src/middlewares/*"],
      "@exceptions/*": ["src/exceptions/*"],
      "@models/*": ["src/models/*"],
      "@services/*": ["src/services/*"]
    }
  },
  "module": {
    "type": "es6"
  }
}
